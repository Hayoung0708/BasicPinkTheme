{
    "name": "basic-pink-theme",
    "displayName": "Basic Pink Theme",
    "description": "A basic theme with soft pink accents and improved readability for Korean text.",
    "version": "1.0.0",
    "publisher": "Hayoung Kang",
    "repository": {
    "type": "git",
    "url": "https://github.com/Hayoung0708/BasicPinkTheme"
    },
    "engines": {
      "vscode": "^1.50.0"
    },
    "categories": ["Themes"],
    "contributes": {
      "themes": [
        {
          "label": "Basic Dark Pink",
          "uiTheme": "vs-dark",
          "path": "./themes/dark_editor_text_color.json"
        },
        {
          "label": "Basic Light Pink",
          "uiTheme": "vs",
          "path": "./themes/light_editor_text_color.json"
        }
      ],
      "configurationDefaults": {
      "editor.fontFamily": "'Menlo', 'Monaco', 'Courier New', 'monospace', '나눔고딕'",
      "editor.fontSize": 16,
      "editor.letterSpacing": 0.2,
      "editor.bracketPairColorization.enabled": false
    }
    }
}
  